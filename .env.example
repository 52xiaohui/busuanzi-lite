# Server Configuration
PORT=3000
NODE_ENV=development

# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379

# Security
ADMIN_PASSWORD=your-admin-password  # 管理界面密码
JWT_SECRET=your-jwt-secret-key      # JWT 加密密钥，建议使用随机字符串

# Rate Limiting
RATE_LIMIT_MAX=120          # 每个时间窗口的最大请求数
RATE_LIMIT_WINDOW_MS=60000  # 时间窗口大小（毫秒）

# Data Retention
LOG_RETENTION_DAYS=30       # 日志保留天数